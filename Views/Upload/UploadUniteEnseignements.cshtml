@{
    ViewData["Title"] = "Importer Unités d'Enseignement";
}

<h2>Importer Unités d'Enseignement</h2>

<form asp-action="UploadUniteEnseignements" enctype="multipart/form-data" method="post">
    <div class="form-group mb-3">
        <label>Fichier CSV (CodeUniteEnseignement, Credits)</label>
        <input type="file" name="file" class="form-control" required />
    </div>
    <button type="submit" class="btn btn-primary">Importer</button>
</form>

<div class="mt-3">
    <a asp-action="Index">Retour</a>
</div>