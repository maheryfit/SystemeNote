@model IEnumerable<SystemeNote.Models.Administrateur>

@{
    ViewData["Title"] = "Administrators";
}

<h1>@ViewData["Title"]</h1>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
    <a asp-action="Upload" class="btn btn-secondary">Import Administrators</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomAdmin)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PrenomAdmin)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.NomAdmin)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PrenomAdmin)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>